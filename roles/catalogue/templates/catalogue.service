[Unit]
Description = Catalogue Service

[Service]
User=roboshop
Environment={{ lookup('aws_ssm', 'dev.catalogue_service.mongo', region='us-east-1' ) }}
Environment={{ lookup('aws_ssm', 'dev.catalogue_service.host_mongo_url', region='us-east-1' ) }}
ExecStart=/bin/node /app/server.js
SyslogIdentifier=catalogue

[Install]
WantedBy=multi-user.target